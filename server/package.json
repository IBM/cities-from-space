{
  "name": "cities-from-space",
  "description": "A server using Watson Visual Recognition to classify images of cities at night taken from the International Space Station.",
  "version": "0.0.1",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "test-integration": "casperjs test ../test/integration/test.*.js",
    "test-integration-runner": "NODE_ENV=test node casper-runner.js",
    "test": "npm run test-integration-runner",
    "test-unit": "istanbul cover ./node_modules/mocha/bin/_mocha test/unit",
    "lint": "eslint .",
    "autofix": "eslint --fix .",
    "codecov": "npm run test && (codecov || true)"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/IBM/cities-from-space"
  },
  "dependencies": {
    "body-parser": "^1.17.x",
    "cfenv": "^1.0.x",
    "dotenv": "^4.0.0",
    "download": "^7.0.0",
    "express": "^4.15.x",
    "formidable": "*",
    "vcap_services": "^0.3.4",
    "watson-developer-cloud": "^3.4.4"
  },
  "devDependencies": {
    "babel-eslint": "^6.0.4",
    "casperjs": "^1.1.3",
    "codecov": "^1.0.1",
    "eslint": "^3.14.1",
    "eslint-config-google": "^0.7.1",
    "eslint-config-prettier": "^2.1.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-prettier": "^2.0.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.13",
    "prettier": "^1.1.0",
    "supertest": "^1.2.0"
  },
  "engines": {
    "node": ">= 6.9.x",
    "npm": "> 3.10.x"
  }
}
